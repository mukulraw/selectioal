        </activity>
        <activity
            android:name=".SignupLogin"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Login"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Signup"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".ForgotPassword"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".SetProfileImage"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Interest"
            android:theme="@style/AppTheme1" />
