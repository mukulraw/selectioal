    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".bean"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignupLogin"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Login"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Signup"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".ForgotPassword"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".SetProfileImage"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".Interest"
            android:theme="@style/AppTheme1" />
        <activity android:name=".StartTest" />
        <activity android:name=".Test" />
        <activity android:name=".TestResult" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Profile" />
        <activity android:name=".TestResult2" />
        <activity android:name=".Settings" />
        <activity android:name=".Orders"></activity>
    </application>

</manifest>